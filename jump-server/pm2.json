{
    "apps": [
      {
        "name": "jump-server",
        "script": "./app.js",
        "max_memory_restart": "512M",
        "env": {
          "NODE_ENV": "development"
        },
        "env_production": {
          "NODE_ENV": "production"
        },
        "instances": 1,
        "max_restarts" : 3,
        "restart_delay" : 5000,
        "log_date_format" : "YYYY-MM-DD HH:mm Z",
        "log_file"   : "./logs/child.log",
        "error_file" : "./logs/child-err.log",
        "out_file"   : "./logs/child-out.log",
        "pid_file"   : "./logs/child.pid"
      }
    ]
  }